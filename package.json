    "dev:back": "./node_modules/.bin/webpack --config ./webpack/webpack-node.config.js --env development --colors --watch",
    "dev:serve": "nodemon --exec babel-node --inspect=9444 server.js",
    "build": "run-p build:*",
    "build:front": "./node_modules/.bin/webpack --config ./webpack/webpack-prod.config.js --colors",
    "build:back": "./node_modules/.bin/webpack --config ./webpack/webpack-node.config.js --env production --colors --verbose --progress",
    "OLDheroku-postbuild": "yarn run build",
    "heroku-postbuild": "",
    "pushall": "git add --all . && git commit -m \"update\" && git push",
    "ncu": "npm-check-updates",
    "local-heroku": "export HEROKU_APP=perkypat-node-static",
    "console-heroku": "heroku run bash"
  },

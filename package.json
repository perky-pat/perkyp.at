{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "^8.8.1",
    "npm": "^5.4.2",
    "yarn": "^1.2.1"
  },
  "main": "server/index.js",
  "scripts": {
    "start": "node ./server/index.js",
    "dev": "run-p dev:*",
    "dev:front": "babel-node ./node_modules/.bin/webpack-dev-server --config ./webpack/webpack-dev.config.js --colors",
    "dev:back": "babel-node ./node_modules/.bin/webpack --config ./webpack/webpack-node.config.js --env development --colors --watch",
    "dev:serve": "nodemon --exec babel-node --inspect server/index.js",
    "build": "run-p build:*",
    "build:front": "babel-node ./node_modules/.bin/webpack --config ./webpack/webpack-prod.config.js --colors",
    "build:back": "babel-node ./node_modules/.bin/webpack --config ./webpack/webpack-node.config.js --env production --colors --verbose --progress",
    "heroku-postbuild": "yarn run build",
    "pushall": "git add --all . && git commit -m \"update\" && git push",
    "ncu": "npm-check-updates"
  },
  "author": "@taseenb",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "css-loader": "^0.28.7",
    "debug": "^3.1.0",
    "ejs": "^2.5.7",
    "express": "^4.15.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "glslify": "^6.1.0",
    "glslify-loader": "^1.0.2",
    "html-loader": "^0.5.1",
    "html-to-react-components": "^1.5.1",
    "minivents": "^2.2.0",
    "modernizr": "^3.5.0",
    "modernizr-loader": "^1.0.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npm-check-updates": "^2.12.1",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "randomcolor": "^0.5.3",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.6",
    "source-map-support": "^0.5.0",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "underscore": "^1.8.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.0",
    "webpack-node-externals": "^1.6.0",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "devDependencies": {},
  "standard": {
    "env": {
      "browser": true,
      "node": true,
      "jquery": true,
      "es6": true,
      "commonjs": true
    },
    "globals": [
      "$",
      "THREE",
      "TweenMax",
      "TimelineMax",
      "Power0",
      "Power1",
      "Power2",
      "Power3",
      "Power4",
      "Linear",
      "Modernizr"
    ]
  }
}

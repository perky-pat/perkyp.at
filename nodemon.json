{
  "verbose": true,
  "ignore": [
    ".git",
    "node_modules"
  ],
  "watch": [
    "src/server/",
    "src/data/"
  ],
  "execMap": {
    "js": "node --trace-sync-io --inspect"
  },
  "delay": "0ms",
  "env": {
    "NODE_ENV": "development",
    "DEBUG": "app,app:*",
    "DEBUG_COLORS": 1
  },
  "ext": "js,json,html,ejs"
}